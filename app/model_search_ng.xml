<div class="row" ng-app="vehicleModelSearchApp">
<script type="text/ng-template" id="disclaimer.html">
<p><small>* MSRP includes destination and handling, but excludes applicable taxes, title, registration, license, options and dealer fees. Regional MSRP pricing may vary.</small></p>
</script>
<script type="text/ng-template" id="main.html">
<ul class="breadcrumbs">
  <li><select class="no-margin" style="width: initial;" ng-model="base.yearSelected" ng-options="obj.yearlabel for obj in base.yearArray"></select></li>
  <li ng-if="makes.makeSelected"><select class="no-margin" style="width: initial;" ng-model="makes.makeSelected" ng-options="obj.make for obj in makes.collection"></select></li>
  <li>Model</li>
  <li>Trim</li>
  <li>Color</li>
</ul>
<h2 class="header2">Select a Model</h2>
<ul class="small-block-grid-2 large-block-grid-4">
  <li ng-repeat="ele in baseF.models">
    <table><tbody>
      <tr><td colspan="2"><a href="#/model/{{ele.model}}" data-snowplow="model-search:add:model:{{ele.model}}"><img ng-src="{{base.ttsrc}}http://cdn.wpdlr.co/assets/vehicles/evox/ext_color/{{ele.evox_vif}}/{{ele.evox_vif}}_cc0640_001_{{ele.ext_color_code}}.png&w=230&h=110&f=2"></a></td></tr>
      <tr>
        <td><strong>{{ele.modelLabel}}</strong>
          <br><font class="font-size-70em">Starts at ${{ele.msrpmodel}}<sup>*</sup></font></td>
        <td class="text-center primary"><span ng-if="ele.modelCount">{{ele.modelCount}}
          <br><font class="font-size-70em">in Stock</font></span></td>
      </tr>
    </tbody></table>
  </li>
</ul>
<div ng-include="'disclaimer.html'"></div>
</script>
<script type="text/ng-template" id="trim.html">
<ul class="breadcrumbs">
  <li><a href="#" data-snowplow="model-search:removed:model:{{subase.current.model}}">{{base.yearSelected.year}}</a></li>
  <li ng-if="makes.makeSelected"><a href="#" data-snowplow="model-search:removed:model:{{subase.current.model}}">{{makes.dealerMake}}</a></li>
  <li><a href="#" data-snowplow="model-search:removed:model:{{subase.current.model}}">{{subase.current.modelLabel}}</a></li>
  <li>Trim</li>
  <li>Color</li>
</ul>
<h2 class="header2">Select a Trim</h2>
<ul class="small-block-grid-2 large-block-grid-4">
  <li ng-repeat="ele in baseF.modelTrim[subase.ele].trimuniq">
    <table><tbody>
      <tr><td colspan="2"><a href="#/model/{{ele.model}}/trim/{{ele.trim}}" data-snowplow="model-search:add:trim:{{ele.trim}}"><img ng-src="{{base.ttsrc}}http://cdn.wpdlr.co/assets/vehicles/evox/ext_color/{{ele.evox_vif}}/{{ele.evox_vif}}_cc0640_001_{{ele.ext_color_code}}.png&w=230&h=110&f=2"></a></td></tr>
      <tr>
        <td><strong>{{ele.trimLabel}}</strong>
          <br><font class="font-size-70em">Starts at ${{ele.msrptrim}}<sup>*</sup></font></td>
        <td class="text-center primary"><span>{{ele.trimCount}}
          <br><font class="font-size-70em">in Stock</font></span></td>
      </tr>
    </tbody></table>
  </li>
</ul>
<div ng-include="'disclaimer.html'"></div>
</script>
<script type="text/ng-template" id="color.html">
<ul class="breadcrumbs">
  <li><a href="#" data-snowplow="model-search:removed:model,trim:{{subase.current.model}},{{subase.current.trim}}">{{base.yearSelected.year}}</a></li>
  <li ng-if="makes.makeSelected"><a href="#" data-snowplow="model-search:removed:model,trim:{{subase.current.model}},{{subase.current.trim}}">{{makes.dealerMake}}</a></li>
  <li><a href="#" data-snowplow="model-search:removed:model,trim:{{subase.current.model}},{{subase.current.trim}}">{{subase.current.modelLabel}}</a></li>
  <li><a href="#/model/{{subase.modelsel}}" data-snowplow="model-search:removed:trim:{{subase.current.trim}}">{{subase.current.trimLabel}}</a></li>
  <li>Color</li>
</ul>
<h2 class="header2">Select a Color</h2>
<ul class="small-block-grid-2 large-block-grid-4">
  <li ng-repeat="ele in baseF.modelTrimColor[subase.ele].colors">
    <table><tbody>
      <tr><td colspan="2"><a href="#/model/{{ele.model}}/trim/{{ele.trim}}/color/{{ele.ext_color_code}}" data-snowplow="model-search:add:color:{{ele.ext_color_code}}"><img ng-src="{{base.ttsrc}}http://cdn.wpdlr.co/assets/vehicles/evox/ext_color/{{ele.evox_vif}}/{{ele.evox_vif}}_cc0640_001_{{ele.ext_color_code}}.png&w=230&h=110"></a></td></tr>
      <tr>
        <td><strong>{{ele.exterior_color}}</strong>
          <br><font class="font-size-70em">Starts at ${{ele.msrp}}<sup>*</sup></font></td>
        <td class="text-center primary"><span>{{ele.colorCount}}
          <br><font class="font-size-70em">in Stock</font></span></td>
      </tr>
    </tbody></table>
  </li>
</ul>
<div ng-include="'disclaimer.html'"></div>
</script>
<script type="text/ng-template" id="quote.html">
<div class="row">
  <div class="columns large-6">
    <ul class="pricing-table">
      <li class="bullet-item">Year: <strong>{{subase.current.year}}</strong></li>
      <li class="bullet-item">Model: <strong>{{subase.current.modelLabel}}</strong></li>
      <li class="bullet-item">Trim: <strong>{{subase.current.trimLabel}}</strong></li>
      <li class="bullet-item">Color: <strong>{{subase.current.exterior_color}}</strong></li>
      <li class="price">MSRP: $<strong>{{subase.current.msrp}}</strong><sup>*</sup></li>
      <li><img ng-src="{{base.ttsrc}}http://cdn.wpdlr.co/assets/vehicles/evox/ext_color/{{subase.current.evox_vif}}/{{subase.current.evox_vif}}_cc0640_001_{{subase.current.ext_color_code}}.png&w=640&h=320"></li>
    </ul>
  </div>
  <div class="columns large-6">
    <ul class="breadcrumbs">
      <li><a href="#" data-snowplow="model-search:removed:model,trim,color:{{subase.current.model}},{{subase.current.trim}},{{subase.current.ext_color_code}}">{{base.yearSelected.year}}</a></li>
      <li ng-if="makes.makeSelected"><a href="#" data-snowplow="model-search:removed:model,trim,color:{{subase.current.model}},{{subase.current.trim}},{{subase.current.ext_color_code}}">{{makes.dealerMake}}</a></li>
      <li><a href="#" data-snowplow="model-search:removed:model,trim,color:{{subase.current.model}},{{subase.current.trim}},{{subase.current.ext_color_code}}">{{subase.current.modelLabel}}</a></li>
      <li><a href="#/model/{{subase.modelsel}}" data-snowplow="model-search:removed:trim,color:{{subase.current.trim}},{{subase.current.ext_color_code}}">{{subase.current.trimLabel}}</a></li>
      <li><a href="#/model/{{subase.modelsel}}/trim/{{subase.trimsel}}" data-snowplow="model-search:removed:color:{{subase.current.ext_color_code}}">{{subase.current.exterior_color}}</a></li>
    </ul>
    <form class="mform" ng-submit="snowpush()" action="/#/" name="mquo" method="post" id="{{subase.element_hash}}">
      <div class="panel radius"><div class="row" style="line-height:2em">
        <div class="columns large-6">
          <label class="left small-5">Quote Type</label>
          <select class="selecth small-5" id="qtype" name="quote_type" ng-model="mform.qtypeSelected">
            <option value="lease">Lease</option>
            <option value="finance">Finance</option>
            <option value="purchase">Purchase</option>
          </select>
        </div>
        <div ng-if="mform.qtypeSelected !== 'purchase'" class="columns large-6">
          <label class="left small-5">Credit Rating</label>
          <select class="selecth small-5" name="credit_rating" ng-model="mform.creditRating">
            <option value="great">Great</option>
            <option value="good">Good</option>
            <option value="soso">So-so</option>
            <option value="notgood">Not so good</option>
          </select>
        </div>
      </div></div>
      <!-- quote type / credit rating -->
      <div class="row">
        <div class="columns small-4">
          <label for="first_name">First Name<span ng-show="mquo.first_name.$error.required"> * </span></label>
          <input type="text" id="first_name" name="first_name" ng-model="mform.firstName" required>
        </div>
        <div class="columns small-4">
          <label for="last_name">Last Name<span ng-show="mquo.last_name.$error.required"> * </span></label>
          <input type="text" id="last_name" name="last_name" ng-model="mform.lastName" required>
        </div>
        <div class="columns small-4">
          <label for="email">Email<span ng-show="mquo.email.$error.required"> * </span></label>
          <input type="email" id="email" name="email" ng-model="mform.email" required>
        </div>
      </div>
      <!-- Personal Information -->
      <input type="hidden" name="year" value="{{subase.current.year}}">
      <input type="hidden" name="model" value="{{subase.current.modelLabel}}">
      <input type="hidden" name="trim" value="{{subase.current.trimLabel}}">
      <input type="hidden" name="color" value="{{subase.current.exterior_color}}">
      <input type="hidden" name="msrp" value="{{subase.current.msrp}}">
      <!-- Model Information -->
      <div ng-if="mform.qtypeSelected === 'lease'" class="row">
        <div class="columns small-4 large-4">
          <label for="lease_term">Term</label>
          <select class="selecth" id="lease_term" name="lease_term" ng-model="mform.leaseTerm">
            <option value="any">Any</option>
            <option value="24">24 mo.</option>
            <option value="36">36 mo.</option>
            <option value="39">39 mo.</option>
            <option value="42">42 mo.</option>
            <option value="48">48 mo.</option>
          </select>
        </div>
        <div class="columns small-4 large-4">
          <label for="miles_year">Miles per year</label>
          <select class="selecth" id="miles_year" name="miles_year" ng-model="mform.milesYear">
            <option value="10000">10,000 mi.</option>
            <option value="12000">12,000 mi.</option>
            <option value="15000">15,000 mi.</option>
            <option value="18000">18,000 mi.</option>
            <option value="20000">20,000 mi.</option>
          </select>
        </div>
        <div class="columns small-12 large-4">
          <label for="max_down">Max down-payment<span ng-show="mquo.max_down.$error.required"> * </span></label>
          <input type="number" id="max_down" name="max_down" ng-model="mform.maxDown" required>
        </div>
        <div class="columns large-12">
          <label for="additional_equipment">Additional Equipment/Notes/Details</label>
          <textarea name="additional_equipment" ng-model="mform.additionalEquipment"></textarea>
        </div>
      </div>
      <div ng-if="mform.qtypeSelected === 'finance'" class="row">
        <div class="columns small-6">
          <label for="finance_term">Term</label>
          <select class="selecth" id="finance_term" name="finance_term" ng-model="mform.financeTerm">
            <option value="any">Any</option>
            <option value="24">24 mo.</option>
            <option value="48">48 mo.</option>
            <option value="72">72 mo.</option>
          </select>
        </div>
        <div class="columns small-6">
          <label for="max_down">Max down-payment<span ng-show="mquo.max_down.$error.required"> * </span></label>
          <input type="number" id="max_down" name="max_down" ng-model="mform.maxDown" required>
        </div>
        <div class="columns large-12">
          <label for="additional_equipment">Additional Equipment</label>
          <textarea name="additional_equipment" ng-model="mform.additionalEquipment"></textarea>
        </div>
      </div>
      <div ng-if="mform.qtypeSelected === 'purchase'" class="row">
        <div class="columns large-12">
          <label for="additional_equipment">Additional Equipment</label>
          <textarea name="additional_equipment" ng-model="mform.additionalEquipment"></textarea>
        </div>
      </div>
      <!-- Quote Type -->
      <input type="hidden" name="dForm-ID" value="{{subase.element_hash}}">
      <input type="hidden" name="dForm-Subject" value="Model Quote">
      <div class="text-right">
        <span ng-hide="mquo.$valid" class="radius alert label">Please Fill all the Fields</span>
        <input class="button small" ng-if="mquo.$valid" type="submit" name="dForm-Submit" value="Submit" data-snowplow="webform:form-submit:instock-quote:{{subase.current.year}},{{subase.current.model}},{{subase.current.trim}},{{subase.current.ext_color_code}}:{{subase.current.msrp}}">
      </div>
    </form>
    <div ng-include="'disclaimer.html'"></div>
  </div>
</div>
</script>]]></wp:meta_value>
